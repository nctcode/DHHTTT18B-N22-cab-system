{
  "name": "booking-service",
  "version": "1.0.0",
  "description": "Microservice for cab booking system",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --testPathPattern=tests/ --detectOpenHandles --forceExit",
    "test:watch": "jest --testPathPattern=tests/ --watch",
    "test:unit": "jest tests/unit/",
    "test:notification": "node scripts/test-notification.js",
    "test:events": "node scripts/test-events.js",
    "test:integration": "jest tests/integration/",
    "test:coverage": "jest --testPathPattern=tests/ --coverage",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "format": "prettier --write \"src/**/*.js\" \"tests/**/*.js\"",
    "docker:build": "docker build -t booking-service .",
    "docker:run": "docker run -p 3002:3002 booking-service",
    "docker:compose": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "migrate": "node scripts/migrate.js",
    "seed": "node scripts/seed.js",
    "health": "node scripts/health-check.js"
  },
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.5.0",
    "redis": "^4.6.10",
    "amqplib": "^0.10.3",
    "jsonwebtoken": "^9.0.2",
    "axios": "^1.5.0",
    "dotenv": "^16.3.1",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "express-rate-limit": "^6.10.0",
    "joi": "^17.9.2",
    "winston": "^3.10.0",
    "socket.io": "^4.7.0",
    "uuid": "^9.0.0",
    "geoip-lite": "^1.4.9",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^5.0.0",
    "swagger-jsdoc": "^6.2.8"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "jest": "^29.6.4",
    "supertest": "^6.3.3",
    "eslint": "^8.48.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.28.1",
    "mongodb-memory-server": "^9.1.0",
    "redis-mock": "^0.56.3",
    "prettier": "^3.0.3"
  },
  "keywords": [
    "microservice",
    "booking",
    "taxi",
    "cab",
    "nodejs",
    "express",
    "mongodb",
    "redis",
    "rabbitmq"
  ],
  "author": "IUH Student - Modern System Architecture Design",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}